<h1 class="page-title">Log In</h1>
<div class="signup">
  <%= form_for @user, {url: session_path} do |f| %>
    <%= f.text_field :email, placeholder: "E-mail" %>
    <%= f.password_field :password, placeholder: "Password" %>

    <%= f.submit "Login", class: "btn btn-submit" %>
    
  <% end %> 
</div>