
<% if !current_user %>
  <h1 class="container text-center">Welcome!</h1> 
  <div class="text-center"><%= link_to "Sign up", new_user_path, :class => "btn btn-success btn-lg "%></div>
<% else %>
  <div class="text-center"><%= link_to "Log Out", session_path, :class => "btn btn-warning btn-lg", method: :delete if current_user%></div>
<% end %>